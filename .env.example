# GONZALES TAX PLATFORM - Environment Configuration
# Copy this file to .env and fill in your values

# ===========================================
# APPLICATION
# ===========================================
ENVIRONMENT=development
DEBUG=true
SECRET_KEY=your-secret-key-min-32-characters-here

# ===========================================
# DATABASE
# ===========================================
DATABASE_URL=postgresql://postgres:password@localhost:5432/gonzales_tax

# ===========================================
# REDIS
# ===========================================
REDIS_URL=redis://localhost:6379

# ===========================================
# IRS E-FILE (MeF)
# ===========================================
IRS_MEF_ENV=ats
IRS_EFIN=your-efin-here
IRS_ETIN=your-etin-here

# ===========================================
# ENCRYPTION
# ===========================================
ENCRYPTION_KEY=your-256-bit-encryption-key-here
AWS_KMS_KEY_ID=your-kms-key-id
AWS_REGION=us-east-1

# ===========================================
# JWT AUTHENTICATION
# ===========================================
JWT_SECRET_KEY=your-jwt-secret-key-min-32-chars

# ===========================================
# THIRD-PARTY INTEGRATIONS (Optional)
# ===========================================
# Plaid (Bank Connections)
PLAID_CLIENT_ID=
PLAID_SECRET=
PLAID_ENV=sandbox

# Stripe (Payments)
STRIPE_SECRET_KEY=
STRIPE_PUBLISHABLE_KEY=
STRIPE_WEBHOOK_SECRET=

# Twilio (SMS)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=

# SendGrid (Email)
SENDGRID_API_KEY=
